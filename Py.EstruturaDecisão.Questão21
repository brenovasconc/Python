dinheiro = int(input("Digite o valor:"))
x = dinheiro // 200
print("%d Notas de 200 reais\n"%x)
y = dinheiro - (x*200)
x = y // 100
print("%d Notas de 100 reais\n"%x)
y = y - (x*100)
x = y // 50
print("%d Notas de 50 reais\n"%x)
y = y - (x*50)
x = y // 20
print("%d Notas de 20 reais\n"%x)
y = y - (x*20)
x = y // 10
print("%d Notas de 10 reais\n"%x)
y = y - (x*10)
x = y // 5
print("%d Notas de 5 reais\n"%x)
y = y - (x*5)
x = y // 2
print("%d Notas de 2 reais\n"%x)
y = y - (x*2)
print("%d Notas de 1 real\n"%y)
