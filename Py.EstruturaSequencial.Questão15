qntHora = (int(input("Insira a quantidade de horas trabalhadas:\n")))
valorHora = (float(input("Insira o valor da sua hora:\n")))
bruto = qntHora * valorHora
IR = bruto * 0.11
INSS = bruto * 0.08
SIND = bruto * 0.05
liq = bruto - (IR + INSS + SIND)
print("+ Salario Bruto: R$%d"%bruto)
print("- IR (11) : R$%d"%IR)
print("- INSS (8) : R$%d"%INSS)
print("- Sindicato (5) : R$%d"%SIND)
print("= Salario Liquido: R$%d"%liq)
